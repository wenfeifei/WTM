@model WalkingTec.Mvvm.Demo.ViewModels.不要用中文模型名VMs.不要用中文模型名ImportVM

<wt:form vm="@Model">
    <wt:row align="AlignEnum.Right">
        <wt:downloadTemplateButton vm="@Model" />
    </wt:row>
    <wt:upload field="UploadFileId" label-text="上传模板" upload-type="ExcelFile" />
    <wt:grid vm="ErrorListVM" use-local-data="true" hidden-checkbox="true" hidden-grid-index="true" hidden-panel="true"  height="300"/>
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>